# Copy this file to .env and fill in real values before running locally or deploying.
# Do NOT commit your real .env to git. Keep secrets secret.

# Your Supabase project URL (found in Supabase project settings)
SUPABASE_URL=https://your-project-ref.supabase.co

# Service role key (server-side only). Use this for Edge Function deployments only.
SUPABASE_SERVICE_ROLE_KEY=service_role_key_here

# A shared secret used to verify webhooks from JotForm (choose a long random string)
JOTFORM_WEBHOOK_SECRET=change_me_to_a_long_random_secret

# Supabase anon/public key (used by the Netlify frontend only)
SUPABASE_ANON_KEY=anon_key_here
