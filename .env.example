# Copy this file to .env and fill in real values before running locally or deploying.
# Do NOT commit your real .env to git. Keep secrets secret.

# Your Supabase project URL (found in Supabase project settings)
# MUST have NEXT_PUBLIC_ prefix to be accessible in the browser
NEXT_PUBLIC_SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase anon/public key (used by the frontend)
# MUST have NEXT_PUBLIC_ prefix to be accessible in the browser
NEXT_PUBLIC_SUPABASE_ANON_KEY=anon_key_here

# Service role key (server-side only). Use this for API routes and admin operations.
# DO NOT add NEXT_PUBLIC_ prefix - this should only be used server-side
SUPABASE_SERVICE_ROLE_KEY=service_role_key_here

# A shared secret used to verify webhooks from JotForm (choose a long random string)
JOTFORM_WEBHOOK_SECRET=change_me_to_a_long_random_secret

# JotForm API key for fetching form data
JOTFORM_API_KEY=your_jotform_api_key_here

